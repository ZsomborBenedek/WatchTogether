{
    "name": "Watch Together",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Synchronized video controlling using WebRTC.",
    "permissions": [
        "storage"
    ],
    "background": {
        "page": "background.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_end",
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "images/icon32.png",
            "64": "images/icon64.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "32": "images/icon32.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
    }
}